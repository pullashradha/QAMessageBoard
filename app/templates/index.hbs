<h1 class="page-header">Welcome to the Epicodus Message Board!</h1>
<div class="questions-list">
  {{#each model as |question|}}
    <h2>{{question.question}}</h2>
    <h3>By {{question.questionAuthor}}, {{question.questionDate}}</h3>
  {{/each}}
</div>

{{new-question saveQuestion="saveQuestion"}}
